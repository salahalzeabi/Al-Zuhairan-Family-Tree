{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/(.*)\\.(js|css|png|jpg|jpeg|gif|webp|svg|ico|txt|json|map|woff|woff2)", "destination": "/$1.$2" },
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
